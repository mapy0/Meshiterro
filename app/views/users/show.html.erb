<div>
  <h3><%= @user.name %></h3>
  <%= attachment_image_tag @user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %><!--プロフィール画像反映-->
  <%= image_tag('sample-author1.jpg') %>
  <!--my page 編集画面へのリンク-->
  <% if @user.id == current_user.id %>
  <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <% end %>
</div>
<!--部分テンプレート呼び出し-->
<%= render 'post_images/list', post_images: @post_images %>